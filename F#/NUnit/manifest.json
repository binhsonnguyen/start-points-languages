
{
  "visible_filenames": [
    "HikerTest.fs",
    "Hiker.fs",
    "cyber-dojo.sh"
  ],

  "filename_extension" : ".fs",

  "display_name": "F#, NUnit",

  "unit_test_framework": "nunit",

  "image_name": "cyberdojofoundation/fsharp_nunit",

  "red_amber_green" : [
    "lambda { |output|",
    "  return :red   if /^Errors and Failures:/.match(output)",
    "  return :green if /^Tests run: (\\d*), Errors: 0, Failures: 0/.match(output)",
    "  return :amber",
    "}"
  ]
}
