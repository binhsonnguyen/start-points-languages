{
  "visible_filenames": [
    "hikerTest.js",
    "hiker.js",
    "cyber-dojo.sh",
    ".jshintrc"
  ],

  "filename_extension" : ".js",

  "tab_size": 2,

  "display_name": "Javascript, assert",

  "unit_test_framework": "node",

  "image_name": "cyberdojofoundation/javascript-node_assert",

  "red_amber_green" : [
    "lambda { |output|",
    "  return :red   if /AssertionError/.match(output)",
    "  return :green if /^All tests passed/.match(output)",
    "  return :amber",
    "}"
  ]
}
