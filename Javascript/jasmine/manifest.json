{
  "visible_filenames": [
    "hiker-spec.js",
    "hiker.js",
    "cyber-dojo.sh",
    ".jshintrc",
    "jasmine.json"
  ],

  "filename_extension": ".js",

  "tab_size": 2,

  "unit_test_framework": "javascript_jasmine",

  "display_name": "Javascript, jasmine",

  "image_name": "cyberdojofoundation/javascript-node_jasmine",

  "progress_regexs": [
    "/(\d+) specs?, ([1-9]\d*) failures?/",
    "/(\d+) specs?, (0) failures?/"
  ],

  "red_amber_green" : [
    "lambda { |output|",
    "  pattern = /(\\d+) specs?, (\\d+) failures?/",
    "  if match = pattern.match(output)",
    "    match[2] == '0' ? :green : :red",
    "  else",
    "    :amber",
    "  end",
    "}"
  ]
}
