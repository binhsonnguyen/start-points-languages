
{
  "visible_filenames": [
    "HikerTest.groovy",
    "Hiker.groovy",
    "cyber-dojo.sh"
  ],

  "filename_extension" : ".groovy",

  "display_name": "Groovy, JUnit",

  "unit_test_framework": "junit",

  "image_name": "cyberdojofoundation/groovy_junit",

  "red_amber_green" : [
    "lambda { |output|",
    "  return :red   if /^Tests run: (\\d*),(\\s)+Failures: (\\d*)/.match(output)",
    "  return :green if /^OK \\((\\d*) test/.match(output)",
    "  return :amber",
    "}"
  ]
}
