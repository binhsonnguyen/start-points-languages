{
  "visible_filenames": [
    "untitledTest.cpp",
    "AllTests.cpp",
    "cyber-dojo.sh",
    "untitled.c",
    "untitled.h",
    "makefile"
  ],

  "highlight_filenames": [
    "untitled.c",
    "untitled.h",
    "untitledTest.cpp"
  ],

  "filename_extension" : ".c",

  "display_name": "C (gcc), CppUTest",

  "image_name": "cyberdojofoundation/gcc_cpputest",

  "progress_regexs" : [
    "Errors \(.* ms\)",
    "OK \(.* ms\)"
  ],

  "red_amber_green" : [
    "lambda { |output|",
    "  return :red   if /Errors \\((\\d+) failures, (\\d+) tests/.match(output)",
    "  return :green if /OK \\((\\d+) tests, (\\d+) ran/.match(output)",
    "  return :amber",
    "}"
  ]
}
